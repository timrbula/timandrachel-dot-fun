<svg width="100" height="100" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      @keyframes twinkle {
        0%, 100% { opacity: 0.3; }
        50% { opacity: 1; }
      }
      .building {
        fill: #2C3E50;
        stroke: #1A252F;
        stroke-width: 1;
      }
      .window {
        fill: #F4D03F;
        animation: twinkle 3s ease-in-out infinite;
      }
      .sky {
        fill: #34495E;
      }
      .antenna {
        stroke: #E74C3C;
        stroke-width: 1;
      }
    </style>
  </defs>
  
  <!-- Sky background -->
  <rect class="sky" x="0" y="0" width="100" height="100"/>
  
  <!-- Stars -->
  <circle cx="10" cy="10" r="1" fill="white" opacity="0.8"/>
  <circle cx="25" cy="15" r="0.5" fill="white" opacity="0.6"/>
  <circle cx="40" cy="8" r="1" fill="white" opacity="0.7"/>
  <circle cx="60" cy="12" r="0.5" fill="white" opacity="0.8"/>
  <circle cx="75" cy="18" r="1" fill="white" opacity="0.6"/>
  <circle cx="90" cy="10" r="0.5" fill="white" opacity="0.9"/>
  
  <!-- Building 1 (left) -->
  <rect class="building" x="5" y="60" width="15" height="40"/>
  <rect class="window" x="7" y="65" width="2" height="3" style="animation-delay: 0s"/>
  <rect class="window" x="11" y="65" width="2" height="3" style="animation-delay: 0.5s"/>
  <rect class="window" x="16" y="65" width="2" height="3" style="animation-delay: 1s"/>
  <rect class="window" x="7" y="72" width="2" height="3" style="animation-delay: 1.5s"/>
  <rect class="window" x="11" y="72" width="2" height="3" style="animation-delay: 0.3s"/>
  <rect class="window" x="16" y="72" width="2" height="3" style="animation-delay: 0.8s"/>
  
  <!-- Building 2 (Empire State inspired) -->
  <rect class="building" x="22" y="35" width="18" height="65"/>
  <rect class="building" x="25" y="30" width="12" height="5"/>
  <line class="antenna" x1="31" y1="30" x2="31" y2="20"/>
  <circle fill="#E74C3C" cx="31" cy="20" r="2"/>
  <rect class="window" x="25" y="40" width="2" height="3" style="animation-delay: 0.2s"/>
  <rect class="window" x="29" y="40" width="2" height="3" style="animation-delay: 0.7s"/>
  <rect class="window" x="33" y="40" width="2" height="3" style="animation-delay: 1.2s"/>
  <rect class="window" x="25" y="50" width="2" height="3" style="animation-delay: 0.9s"/>
  <rect class="window" x="29" y="50" width="2" height="3" style="animation-delay: 0.4s"/>
  <rect class="window" x="33" y="50" width="2" height="3" style="animation-delay: 1.4s"/>
  
  <!-- Building 3 (middle) -->
  <rect class="building" x="42" y="50" width="16" height="50"/>
  <rect class="window" x="45" y="55" width="2" height="3" style="animation-delay: 0.6s"/>
  <rect class="window" x="49" y="55" width="2" height="3" style="animation-delay: 1.1s"/>
  <rect class="window" x="53" y="55" width="2" height="3" style="animation-delay: 0.1s"/>
  <rect class="window" x="45" y="65" width="2" height="3" style="animation-delay: 1.3s"/>
  <rect class="window" x="49" y="65" width="2" height="3" style="animation-delay: 0.8s"/>
  <rect class="window" x="53" y="65" width="2" height="3" style="animation-delay: 0.3s"/>
  
  <!-- Building 4 (One World Trade inspired) -->
  <polygon class="building" points="60,45 78,45 76,100 62,100"/>
  <rect class="window" x="64" y="50" width="2" height="3" style="animation-delay: 0.5s"/>
  <rect class="window" x="68" y="50" width="2" height="3" style="animation-delay: 1s"/>
  <rect class="window" x="72" y="50" width="2" height="3" style="animation-delay: 0.2s"/>
  <rect class="window" x="64" y="60" width="2" height="3" style="animation-delay: 1.5s"/>
  <rect class="window" x="68" y="60" width="2" height="3" style="animation-delay: 0.7s"/>
  <rect class="window" x="72" y="60" width="2" height="3" style="animation-delay: 1.2s"/>
  <line class="antenna" x1="69" y1="45" x2="69" y2="35"/>
  
  <!-- Building 5 (right) -->
  <rect class="building" x="80" y="65" width="15" height="35"/>
  <rect class="window" x="82" y="70" width="2" height="3" style="animation-delay: 0.4s"/>
  <rect class="window" x="86" y="70" width="2" height="3" style="animation-delay: 0.9s"/>
  <rect class="window" x="90" y="70" width="2" height="3" style="animation-delay: 1.4s"/>
  <rect class="window" x="82" y="80" width="2" height="3" style="animation-delay: 0.6s"/>
  <rect class="window" x="86" y="80" width="2" height="3" style="animation-delay: 1.1s"/>
  <rect class="window" x="90" y="80" width="2" height="3" style="animation-delay: 0.1s"/>
</svg>